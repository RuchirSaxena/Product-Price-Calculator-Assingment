    },
    "transform": {
      "^.+\\.(js)$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "repository": "",
